package overriding;

public class Animal {
	
	public String howl;
	public String bark;
	public String jump;
	
	
	public Animal(String n, String p, String o) {
		howl = n;
		bark = p; //모든웹툰은 첫 회차가 1회차이므로
		jump = o;

		
		System.out.println(howl + " 울기: " + bark + "물기 : " + jump + "점픔 : ");
	
	}

}
