package game.Interraction;

public class Warrior {

	public static void main(String[] args) {
		
		
		private int hp;
		private int mp;
		private int atk;
		private int def;
		private int exp;
		
		public Warrior(String name) {
			this.hp = 20;
			this.mp = 10;
			this.atk = 4;
			this.def = 1;
			this.exp = 0;
		

	}

}
