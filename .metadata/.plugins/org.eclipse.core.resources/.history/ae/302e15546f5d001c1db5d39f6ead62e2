package game.inheritance;

public class Main03 {
	
	public static void main(String[] args) {
		
		// Warrior 생서을 하나 해주세요.
		
		Magician m1 = new Magician("마법사");
		// 정보를 조회 해보시고, 호출순번도 고려해보세요.
		m1.getInfo();
		
		Archer A1 = new Archer("아쳐");
		// 정보를 조회 해보시고, 호출순번도 고려해보세요.
		A1.getInfo();
	 }
}


